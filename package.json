{
	"name": "nodejsscript",
	"version": "0.5.1",
	"author": "Jan Andrle <andrle.jan@centrum.cz>",
	"license": "MIT",
	"description": "A tool for writing better scripts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaandrle/nodejsscript.git"
	},
	"bugs": {
		"url": "https://github.com/jaandrle/nodejsscript/issues"
	},
	"homepage": "https://github.com/jaandrle/nodejsscript",
	"main": "index.js",
	"type": "module",
	"bin": {
		"nodejsscript": "bin/cli.mjs"
	},
	"exports": {
		".": "./index.js"
	},
	"files": [
		"bin/cli.mjs",
		"index.js"
	],
	"types": "types/index.d.ts",
	"typesVersions": {
		"*": {
			".": [
				"./types/index.d.ts"
			]
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"engines": {
		"node": ">= 16.0.0"
	},
	"keywords": [
		"cli",
		"nodejs"
	],
	"jshintConfig": {
		"esversion": 11,
		"node": "true",
		"module": true,
		"undef": "true",
		"latedef": "true",
		"maxparams": 4,
		"maxdepth": 4,
		"maxcomplexity": 11
	},
	"dependencies": {
		"@types/sade": "^1.7.4",
		"@types/shelljs": "^0.8.11",
		"ansi-colors": "^4.1.3",
		"node-fetch": "^3.2.10",
		"sade": "^1.8.1",
		"shelljs": ">=v0.8.5"
	}
}
